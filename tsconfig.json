{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "skipLibCheck": true,
    "types": ["astro/client", "google.maps"],
    "paths": {
      "src/*": ["src/*"],
      "@/*": ["src/*"]
    }
  }
}